import java.util.Scanner;

public class project {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int number= input.nextInt();
        int sum = 0,r;

        while(number>0){
            r = number%10;
            sum += r;
            number /= 10;
        }

        System.out.print(sum);
    }
}
