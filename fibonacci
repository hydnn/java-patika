import java.util.Scanner;

public class project {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int a=1, b=1, c;
        int n = input.nextInt();

        System.out.print("0 ");
        System.out.print(a + " ");
        System.out.print(b + " ");

        for(int i=1; i<=n; i++){
            c = a + b;
            a = b;
            b = c;

            System.out.print(c+" ");
        }
    }
}
