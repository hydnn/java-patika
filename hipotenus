import java.util.Scanner;

public class Main {
  public static void main(String[] args){
    int a,b;
    double c;
    
    Scanner input = new Scanner(System.in);
    
    System.out.print("kenar gir : ");
    a = input.nextInt();
    System.out.print("kenar gir : ");
    b = input.nextInt();
   
   c = Math.sqrt((a*a) + (b*b));
   System.out.println("Hipoten√ºs : " + c);
  }
}
