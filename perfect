import java.util.Scanner;

public class project {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n, i=1, sum=0;
        n = input.nextInt();

        while(i<n){
            if(n%i==0){
                sum += i ;
            }
            i++;
        }

        if(sum==n){
            System.out.print("perfect");
        }else{
            System.out.print("not perfect");
        }
    }
}
