import java.util.Scanner;

class project{
    static int pow(int n, int x) {
        if (x == 0) {
            return 1;
        }
        return n * pow(n, x-1);
    }
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        int x = input.nextInt();

        System.out.println(pow(n,x));
    }
}
