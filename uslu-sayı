import java.util.Scanner;

public class project {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int x,y, total=1, i=1;
        x = input.nextInt();
        y = input.nextInt();

        while(i<=y){
            total *= x;
            i++;
        }

        System.out.print(total);
    }
}
